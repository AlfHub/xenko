namespace SiliconStudio.Xenko.Graphics.Tests
{
    partial effect CustomSubEffect
    {
        using params CustomShaderKeys;

		if (CustomShaderKeys.SwitchEffectLevel < 10)
		{
			mixin CustomShader;
		}
		else
		{
			mixin CustomShader2;
		}
    };

    /// <summary>
    /// A gaussian blur effect
    /// </summary>
    effect CustomEffect
    {
        mixin CustomShader;
        mixin child CustomSubEffect;
    };
}