<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <!--
    We need both XenkoNativePathLibs and XenkoNativePathLibs2 as in Windows platforms it's a simple string while on other platforms they need to be iterated and processed
  -->
  <PropertyGroup>
    <XenkoNativePathLibs Condition="'$(SiliconStudioPlatform)' == 'Windows'">LibOVR.lib</XenkoNativePathLibs>
  </PropertyGroup>
  
  <ItemGroup>
    <XenkoNativePathLibs2 Condition="'$(SiliconStudioPlatform)' == 'iOS'" Include="$(XenkoDependenciesDir)\GoogleVR\lib\iOS\libgvr.a;" />
  </ItemGroup>
</Project>
